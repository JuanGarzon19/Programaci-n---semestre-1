#include<iostream>
#include<math.h>

using namespace std;

int main()
{
    float a,b,x,y,z;
    cout<<"ingrese el valor de x: "; 
    cin>>x;
    cout<<"ingrese el valor de y: ";
    cin>>y;
    
    a = (sqrt(x + pow(y,2)/3+x));
    
    b = pow(x,2)/(pow(y,3)+x) - 5*(sqrt(3*x));
    
    z=a-b;
    cout<<"El resultado de la formula es de: "<<z<<endl;
    return 0;
}
